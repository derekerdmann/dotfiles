{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "segments": [
        {
          "type": "status",
          "style": "powerline",
          "invert_powerline": true,
          "foreground": "transparent",
          "background": "2",
          "background_templates": [
            "{{ if .Error }}1{{ end }}"
          ],
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true,
            "status_template": "{{ if eq .Code 0 }}\uf00c{{ else }}{{ .Code }} \uf071{{ end }}"
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true,
          "foreground": "0",
          "background": "3",
          "template": " {{ .FormattedMs }} ",
          "powerline_symbol": "\ue0b2",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": false
          }
        },
        {
          "type": "text",
          "style": "powerline",
          "foreground": "transparent",
          "background": "transparent",
          "template": "\u00a0"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "7",
          "foreground": "232",
          "style": "powerline",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "1",
          "foreground": "232",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "4",
          "foreground": "254",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "2",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}3{{ end }}",
            ""
          ],
          "foreground": "0",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
      "background": "transparent",
      "foreground": "240",
      "template": "\u276f "
  },
  "final_space": true,
  "version": 2
}
