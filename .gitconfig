[user]
	name = Derek Erdmann
	email = derek@derekerdmann.com
[alias]
    st = status
    glog = log --graph --oneline
    com = commit    
    last = log -1
[core]
	editor = gvim
    excludesfile = ~/.gitignore-global
[color]
	ui = true
[push]
	default = upstream
[url "git@github.com:"]
  insteadOf = gh:
